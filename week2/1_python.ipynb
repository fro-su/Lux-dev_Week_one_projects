    "import numpy as np\n",
    "\n",
    "arr = np.array([10,20,30])\n",
    "# sum the array elements with sum() function\n",
    "sum_arr =arr.sum()\n",
    " \n",
    "print(f\"Sum of array element is:{sum_arr}\")\n",
    "\n",
    "# sum the array elements with for loop\n",
    "for i in arr:\n",
    "  sum=i+i \n",
    "print(f\"Sum of array element is:{sum}\")"
   ]
